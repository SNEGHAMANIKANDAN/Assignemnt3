   "source": [
    "Write a Python program to reverse a string.\n",
    "\n",
    "\n",
    "\n",
    "Sample String : \"1234abcd\"\n",
    "\n",
    "Expected Output : \"dcba4321\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "c530513c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the string to be reversed\n",
      "snegha\n",
      "the reverse string is\n",
      "ahgens\n"
     ]
    }
   ],
   "source": [
    "def revstr(s):\n",
    "    return s[::-1]\n",
    "\n",
    "\n",
    "print(\"Enter the string to be reversed\")\n",
    "string=input()\n",
    "rev=revstr(string)\n",
    "print(\"the reverse string is\")\n",
    "print(rev)\n"
   ]
  },
